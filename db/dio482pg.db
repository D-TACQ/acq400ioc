	field(DTYP, "Soft Channel")
	field(OMSL, "closed_loop")
	field(DOL,  "${UUT}:${SITE}:BYPASS_TRG_DEBOUNCE:r NPP")
	field(OUT,  "${UUT}:${SITE}:BYPASS_TRG_DEBOUNCE PP")
}


