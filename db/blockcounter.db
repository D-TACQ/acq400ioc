record(longin, "${UUT}:${SITE}:STAT:BLEN") {
	field(SCAN, "10 second")
	field(PINI, "YES")
	field(DTYP, "stream")
	field(INP,  "@signal.proto getBLEN ${SPORT}")
}

record(longin, "${UUT}:${SITE}:STAT:GETS:rc") {
	field(SCAN, "1 second")
	field(DTYP, "stream")	
	field(INP,  "@signal.proto getNGETS ${SPORT}")	
	field(DESC, "buffer GETS count")	
	field(FLNK, "${UUT}:${SITE}:STAT:GETS:ACTIVE")
	field(PRIO, "HIGH")
}

record(calc, "${UUT}:${SITE}:STAT:GETS:MBYTES") {
	field(INPA, "${UUT}:${SITE}:STAT:BLEN"
	field(INPB, "${UUT}:${SITE}:STAT:GETS:COUNT")
	field(CALC, "A*B/1000000")
	field(FLNK, "${UUT}:${SITE}:STAT:GETS:MBPS")
}

record(calc, "${UUT}:${SITE}:STAT:GETS:MBPS") {
	field(INPA, "${UUT}:${SITE}:STAT:BLEN")
	field(INPB, "${UUT}:${SITE}:STAT:GETS:FREQ")
	field(CALC, "A*B/1000000")
}

record(calc, "${UUT}:${SITE}:STAT:GETS:FREQ" ){
	field(FLNK, "${UUT}:${SITE}:STAT:GETS:MBYTES")
}

record(longin, "${UUT}:${SITE}:STAT:PUTS:rc") {
	field(SCAN, "I/O Intr")
	field(DTYP, "stream")	
	field(INP,  "@signal.proto getNPUTS ${SPORT}")	
	field(DESC, "buffer PUTS count")	
	field(FLNK, "${UUT}:${SITE}:STAT:PUTS:ACTIVE")
	field(PRIO, "HIGH")
}

