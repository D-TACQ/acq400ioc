record(longin, "${UUT}:${SITE}:AWG:SAMPLES") {
	
}

record(mbbi, "${UUT}:${SITE}:AWG:STATE") {
	field(DTYP, "stream")
	field(NOBT, "2")
	field(ZRST, "IDLE")
	field(ONST, "ARM")
	field(TWST, "RUN")
	field(INP, "@dio432.proto get_dpg(${UUT}:${SITE}:AWG:SAMPLES) ${SPORT}")
	field(SCAN, ".1 second")
}


record(mbbiDirect, "${UUT}:${SITE}:COS:STATUS:L16") {
	field(DTYP, "stream")
	field(INP,  "@dio.proto getCosSta(${UUT}:${SITE}:COS:STATUS, L16, H16) ${SPORT}")
	field(PINI, "YES")
	field(VAL,  "0")
	field(DESC, "Change Of State Status, low 16 chan")
	field(SCAN, ".2 second")
}

record(mbbiDirect, "${UUT}:${SITE}:COS:STATUS:H16") {
	field(DTYP, "Soft Channel")	
	field(PINI, "YES")
	field(VAL,  "0")
	field(DESC, "Change Of State Status, high 16 chan")
}

