# reports mgt temperature.

record(ai, "${UUT}:${SITE}:FPGA:TEMP") {
        field(DTYP, "dsSysfsAi")
        field(INP, "@/etc/acq400/${SITE}/fpga_temp")
        field(SCAN, "2 second")
        field(DESC, "FPGA internal temp")
# 503.975/4096        
        field(ESLO, "0.123")
        field(EOFF, "-273.5")
        field(LINR, "LINEAR")
        field(EGU,  "C")
        field(PINI, "YES")
        field(HOPR, "70")
        field(LOPR, "0")
        field(LOW,  "0")
        field(HIGH, "60")
        field(HIHI, "70")  
        field(PREC, "2")      
}
