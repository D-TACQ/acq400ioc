record(ai, "${UUT}:SYS:TEMP:BB:0") {
	field(DTYP, "dsSysfsAi")
	field(INP, "@/dev/hwmon/0/temp")
	field(SCAN, "10 second")
	field(DESC, "Temperature Base Board 0")
	field(ESLO, 0.001)
	field(LINR, "LINEAR")
	field(EGU, "C")
}

alias("${UUT}:SYS:TEMP:BB:0", "${UUT}:SYS:TEMP:BB:1")
alias("${UUT}:SYS:TEMP:BB:0", "${UUT}:SYS:TEMP:BB:2")

