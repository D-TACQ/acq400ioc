record(waveform, "${UUT}:MGT:HG:${chan}:PULL_DATA") {	
	field(DTYP, "wfAcqHost")
	field(INP, "@/dev/mgt400.${chan}.pull_data_hg")
	field(SCAN, "10 second")		
#	field(SCAN, "I/O Intr")	
	field(DESC, "pull data fifo histogram")
	field(NELM,"${size}")
	field(FTVL,"${type}")	
}

