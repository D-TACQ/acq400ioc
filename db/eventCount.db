record(longin, "${UUT}:1:EVT:INDX:rc") {
	field(SCAN, "I/O Intr")
	field(DTYP, "stream")	
	field(INP,  "@event.proto eventCount ${SPORT}")	
	field(DESC, "Event count")	
	field(PRIO, "HIGH")
}

record(longin, "${UUT}:1:EVT:SMPL:rc") {
	field(SCAN, "I/O Intr")
	field(DTYP, "stream")	
	field(INP,  "@event.proto sampleCount ${SPORT}")	
	field(DESC, "Event count")	
	field(PRIO, "HIGH")
}

record(bi, "${UUT}:${SITE}:TRIGGERED") {
	field(DTYP, "stream")
	field(INP,  "@event.proto isTriggered ${SPORT}")
	field(ZNAM, "NO")
	field(ONAM, "YES")
	field(SCAN, ".2 second")
}

record(ao, "${UUT}:${SITE}:SOD_TIMEOUT") {
	field(DTYP, "Soft Channel")
	field(DESC, "Switch to free running clock on timeout")
	field(DRVL, "0")
	field(DRVH, "100")
}

