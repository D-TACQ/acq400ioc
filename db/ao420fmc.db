        field(DTYP, "dsSysfsAo")
        field(OUT, "@/dev/acq400.${SITE}.knobs/AO_0${cid}")
        field(SCAN, "Passive")
        field(DESC, "Slow set AO${cid}")
        field(DRVL, "-2.5")
        field(DRVH, "+2.5")
        field(LINR, "LINEAR")
        field(ESLO, "0.076293945e-3")
        field(HOPR, "2.5")
