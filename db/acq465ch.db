# acq465 per channel PV's : OFFSET, GAIN.

record(ao, "${SITE}:ACQ465:OFFSET:${CH}") {
	field(DTYP, "stream")
	field(OUT,  "@acq465.proto acq465_set_offset(${CH}) ${SPORT}")
	field(SCAN, "Passive")
	field(DESC, "Offset CAL setting")
	field(DRVL, "-100")
	field(DRVH, "+100")
	field(LINR, "LINEAR")
# +100 = 2**22-1 = 4194304. ESLO=100/4194304	
	field(ESLO, "2.3841857e-5")
	field(HOPR, "100")
	field(LOPR, "-100")
	field(EGU,  "%")
	field(VAL,  "0")
	field(PINI, "YES")
}

record(ao, "${SITE}:ACQ465:GAIN:${CH}") {
	field(DTYP, "stream")
	field(OUT,  "@acq465.proto acq465_set_gain(${CH}) ${SPORT}")
	field(SCAN, "Passive")
	field(DESC, "Gain CAL setting")
	field(DRVL, "0")
	field(DRVH, "+50")
	field(LINR, "LINEAR")
# +50 = 2**20-1 = 1048576  ESLO=50/1048576	
	field(ESLO, "4.7683715e-5")
	field(HOPR, "0")
	field(LOPR, "-50")
	field(EGU,  "%")
	field(VAL,  "0")
	field(PINI, "YES")
}
