}

record(calc, "${UUT}:${chan}:AGGREGATOR:SPAD:LEN") {
	field(INPA, "${UUT}:${chan}:AGGREGATOR:SPAD:EN:r")
	field(INPB, "${UUT}:0:SPAD:LEN:r")
