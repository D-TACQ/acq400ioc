record(longout, "${UUT}:${SITE}:JDG:${CH}:WIN:L")
{
	field(DTYP, "asynInt32")
	field(OUT,  "@asyn($(PORT),$(IX),$(TIMEOUT))WINL")
	field(HOPR, "${NSAM}")
	field(LOPR,  "2")
	field(VAL,   "2")
	field(PINI,  "YES")
}

record(longout, "${UUT}:${SITE}:JDG:${CH}:WIN:R")
{
	field(DTYP, "asynInt32")
	field(OUT,  "@asyn($(PORT),$(IX),$(TIMEOUT))WINR")
	field(HOPR, "${NSAM}")
	field(LOPR,  "2")
	field(VAL,   "${NSAM}")
	field(PINI,  "YES")	
}