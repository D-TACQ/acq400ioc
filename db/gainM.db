# ACQ420, ACQ425 gains : Master : control only

record (mbbo, "${UUT}:${SITE}:GAIN:${CH}") {	
	field(DTYP, "stream")
	field(OUT,  "@acq42x.proto setGainAll ${SPORT}")
	field(NOBT, "2")	
	field(ZRST, "10V")
	field(ONST, "5V")
	field(TWST, "2V5")
	field(THST, "1V25")
	field(FLNK, "${UUT}:${SITE}:GAIN_CHANGES")    	
}

record (calc, "${UUT}:${SITE}:GAIN_CHANGES") {
	field(INPA, "${UUT}:${SITE}:GAIN_CHANGES.LA")
	field(CALC, "A+1")
}