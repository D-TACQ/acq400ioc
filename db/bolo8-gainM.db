# BOLO8

record (mbbo, "${HS}:B8:GAIN:ALL") {	
	field(DTYP, "Soft Channel")
	field(NOBT, "2")	
	field(ZRST, "10V")
	field(ONST, "5V0")
	field(TWST, "2V5")
	field(THST, "1V2") 
	field(FLNK, "${HS}:B8:GAIN:ALL:fan1")
}


record(dfanout, "${HS}:B8:GAIN:ALL:fan1") {
	field(DOL,  "${HS}:B8:GAIN:ALL")
	field(OMSL, "closed_loop")
	field(FLNK, "${HS}:B8:GAIN:ALL:GAIN_CHANGES")
}

record (calc, "${HS}:B8:GAIN:ALL:GAIN_CHANGES") {
	field(INPA, "${HS}:B8:GAIN:ALL:GAIN_CHANGES")
	field(CALC, "A+1")
}