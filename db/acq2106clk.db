record (bi, "${UUT}:SYS:CLK:lol") {
	field(DTYP, "biAcqHost")
	field(INP,  "@/dev/gpio/CLK/LOL")
	field(SCAN, "1 second")
	field(FLNK, "${UUT}:SYS:CLK:LOL")
}

record (calc, "${UUT}:SYS:CLK:LOL" ) {
	field(INPA, "${UUT}:SYS:CLK:lol")
	field(INPB, "${UUT}:SYS:CLK:BYPASS")
	field(CALC, "B==0 && A!=0")
}

