record (stringin, "${UUT}:SYS:CLK:CONFIG") {
	field(DTYP, "stream")
	field(INP,  "@acq2106clk.proto getSi5326config ${SPORT}")
	field(SCAN, "1 second")
}

record (bo, "${UUT}:SYS:CLK:OE_CLK1_ELF1") {
	field(DTYP, "boAcqHost")	
	field(OUT, "@/dev/gpio/CLK/OE_CLK1_ELF1")	
}

