record (stringin, "${UUT}:SYS:CLK:CONFIG") {
	field(DTYP, "stream")
	field(INP,  "@acq2106clk.proto getSi5326config ${SPORT}")
	field(SCAN, "1 second")
}

record (bi, "${UUT}:SYS:CLK:BYPASS" ) {
	field(DTYP, "stream")
	field(INP,  "@acq2106clk.proto getSi5326bypass ${SPORT}")
	field(SCAN, "1 second")
}
