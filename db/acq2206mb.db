record (bi, "${UUT}:0:SYS:FAN_EN") {
	field(DESC, "motherboard fan enable")
	field(DTYP, "biAcqHost")
	field(INP, "@/dev/gpio/FAN_EN")
	field(SCAN, "5 second")
	field(ZNAM, "OFF")
	field(ONAM, "ON")
	field(PINI, "YES")	
}

