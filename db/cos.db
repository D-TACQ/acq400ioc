record(mbbiDirect, "${UUT}:${SITE}:COS:STATUS:L16") {
	field(DTYP, "stream")
	field(INP,  "@dio432.proto getCosSta(${UUT}:${SITE}:COS:STATUS, L16, H16) ${SPORT}")
	field(PINI, "YES")
	field(VAL,  "0")
	field(DESC, "Change Of State Status, low 16 chan")
	field(SCAN, ".5 second")
	field(NOBT, "16")
}

