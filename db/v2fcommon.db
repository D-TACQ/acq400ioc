record(mbbo, "${UUT}:${SITE}:V2F:FREQ_OFFSET") {
	field(DTYP, "stream")
	field(OUT,  "@v2f.proto setFreqOffset ${SPORT}")
	field(NOBT, "2");
}

record(bo, "${UUT}:${SITE}:V2F:HIGH_FREQ") {
	field(DTYP, "stream")
	field(OUT,  "@v2f.proto setHF ${SPORT}")
	field(DESC, "1 selects 10MHz range")
}

record(longout, "${UUT}:${SITE}:V2F:SRC:1") {
	field(DTYP, "Soft Channel")
	field(DRVL, "1")
	field(DRVH, "${NCHAN}")
	field(VAL,  "1")
	field(FLNK, "${UUT}:${SITE}:V2F:SRC:x")
}

