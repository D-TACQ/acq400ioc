}

record (mbbo, "${UUT}:0:SIG:ZCLK_SRC:w") {
	field(DTYP, "stream")
	field(NOBT, "4")
	field(OUT,  "@mb.proto set_zclk_src ${SPORT}")	
	field(PINI, "YES")	
	field(DISV, "1")
	field(SDIS, "${UUT}:0:SIG:ZCLK_SRC:s.PACT")
	field(OMSL, "closed_loop")
	field(DOL,  "${UUT}:0:SIG:ZCLK_SRC.RVAL")	
}

record (mbbi, "${UUT}:0:SIG:ZCLK_SRC:r") {
	field(DTYP, "stream")
	field(NOBT, "4")
	field(ZRST, "INT33M")
	field(ZRVL, "0")
	field(ONST, "${ZCLK_ONST}")
	field(ONVL, "1")
	field(TWST, "CLK.d0")
	field(TWVL, "8")
	field(THST, "CLK.d1")
	field(THVL, "9")
	field(FRST, "CLK.d2")
	field(FRVL, "10")
	field(FVST, "CLK.d3")
	field(FVVL, "11")
	field(SXST, "CLK.d4")
	field(SXVL, "12")
	field(SVST, "CLK.d5")
	field(SVVL, "13")
	field(EIST, "CLK.d6")
	field(EIVL, "14")
	field(NIST, "CLK.d7")
	field(NIVL, "15")
	field(INP,  "@mb.proto get_zclk_src ${SPORT}")
