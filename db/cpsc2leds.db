record(bo, "${UUT}:SIG:LED:1:GREEN") {
        field(DTYP, "boAcqHost")
        field(OUT, "@/dev/gpio/LED/LED1")
        field(DESC, "LED cpsc:1:green:idle")
        field(ZNAM, "OFF")
        field(ONAM, "ON")
        field(PINI, "YES")
}

