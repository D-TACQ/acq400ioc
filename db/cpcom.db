# cpcom.db comms module  CH={1|2}

record(mbbi, "${UUT}:${CH}:AURORA:UP") {
	field(DTYP, "dsSysfsBi")
	field(INP, "@/dev/acq400.${SITE}.knobs/aurora${CH}_lane_up")
	field(ZNAM, "DOWN")
    field(ONAM, "UP")
    field(SCAN, "1 second")
}

record(longin, "${UUT}:${CH}:AURORA:ERR") {
	field(DTYP, "dsSysfsBi")
	field(INP,  "@/dev/acq400.${SITE}.knobs/aurora${CH}_hard_err")
	field(ZNAM, "OK")
    field(ONAM, "ERR")	
	field(SCAN, "1 second")
}
