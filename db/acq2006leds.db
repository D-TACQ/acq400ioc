record (bo, "${UUT}:SYS:LED:TRG") {
	field(ONAM, "ON")
	field(ZNAM, "OFF")
	field(DESC, "$placeholder")
	field(DTYP, "boAcqHost")
	field(SCAN, "1 second")
	field(OMSL, "closed_loop")	
	field(DOL, "${UUT}:0:SIG:TRG_EXT:ACTIVE")
	field(OUT, "@/dev/gpio/LED/TRG_G")
}

