# common file for SAR adc.
record (longout, "${UUT}:${SITE}:ADC_MODE") {
        field(DTYP, "Soft Channel")
        field(VAL,  "-1")  
}

record (bo, "${UUT}:${SITE}:HAS_CH_GAINS") {
	field(DESC, "$placeholder")
	field(DTYP, "Soft Channel")
	field(VAL,  "${CHG}")
	field(ZNAM, "0")
	field(ONAM, "1")	
}

record (mbbo, "${UUT}:${SITE}:ADC_CAL") {
        field(DTYP, "Soft Channel")
        field(NOBT, "2")
        field(ZRST, "default")
        field(ONST, "caldef")
        field(TWST, "calibrated")
	field(VAL,  "0")
	field(PINI, "YES")
}
