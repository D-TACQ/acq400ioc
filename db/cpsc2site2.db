}

record(bi, "${UUT}:2:PACKET_CLEAR:r") {
		field(DTYP, "stream")
		field(INP,  "@cpsc2.proto get_packet_clear ${SPORT}")
		field(SCAN, "1 second")
		field(FLNK, "${UUT}:2:PACKET_CLEAR:s")
} 

record(bo, "${UUT}:2:PACKET_CLEAR:s") {
		field(DTYP, "Soft Channel")
		field(DOL,  "${UUT}:2:PACKET_CLEAR:r NPP")
		field(OMSL, "closed_loop")
		field(OUT,  "${UUT}:2:PACKET_CLEAR PP")
