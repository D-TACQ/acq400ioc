
record(bi, "${UUT}:SYS:pwrgood") {
	field(DTYP, "Soft Channel")
	field(INP,  "1")
	field(DESC, "cpsc card power good signal")
	field(PINI, "YES")	
}

record(bi, "${UUT}:PSU:FAULT") {
	field(DTYP, "biAcqHost")
	field(INP,  "@/dev/gpio/FAULT")
	field(DESC, "PSU chassis global fault")
	field(SCAN, ".1 second")
}

