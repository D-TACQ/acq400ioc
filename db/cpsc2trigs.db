	field(PINI, "YES")	
}

record(bo, "${UUT}:SIG:FSCLK") {
	field(DTYP, "Soft Channel")
	field(DOL,  "${UUT}:0:SIG:CLK_S4:ACTIVE NPP")
	field(OMSL, "closed_loop")
}

record(calc, "${UUT}:0:SIG:CLK_S4:FREQ") {
	field(FLNK, "${UUT}:SIG:FSCLK")
}

