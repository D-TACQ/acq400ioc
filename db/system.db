record (longin, "${UUT}:SYS:UPTIME") {
	field(DTYP, "longinAcqHost")
	field(INP, "@/proc/uptime")
	field(SCAN, "1 second")
}

record (stringin, "${UUT}:SYS:VERSION:SW") {
	field(DTYP, "stream")
	field(INP,  "@mb.proto getSwVersion ${SPORT}")	
	field(PINI, "YES")
}

record (stringin, "${UUT}:SYS:VERSION:FPGA" ) {
	field(DTYP, "stream")
	field(INP,  "@mb.proto getFpgaVersion ${SPORT}")	
	field(PINI, "YES")
}

record (stringin, "${UUT}:SYS:IFCONFIG:ETH0" ) {
	field(DTYP, "stream")
	field(INP,  "@mb.proto getIfconfig(eth0) ${SPORT}")
	field(PINI, "YES")
}

record (bo, "${UUT}:NOALIASES") {
	field(DTYP, "Soft Channel")
}

record (bo, "${UUT}:IOC_READY") {
	field(DTYP, "Soft Channel")
	field(VAL,  "0")
	field(PINI, "YES")
}

record (stringin, "IP:${ipaddrbis}") {
	field(DTYP, "Soft Channel")
	field(INP,  "${UUT}")
	field(PINI, "YES")
	field(DESC  "Reverse DNS feature")
}


