record (longin, "${UUT}:SYS:UPTIME") {
	field(DTYP, "longinAcqHost")
	field(INP, "@/proc/uptime")
	field(SCAN, "1 second")
	field(FLNK, "${UUT}:SYS:IOCUPTIME")
}

record (calc, "${UUT}:SYS:IOCUPTIME") {
	field(CALC, "VAL+1")
	field(FLNK, "${UUT}:SYS:HB")
}

record (calc, "${UUT}:SYS:HB") {
	field(INPA, "${UUT}:SYS:UPTIME")
	field(CALC, "A&3")
}

record (bo, "${UUT}:IOC_READY") {
	field(DESC, "IOC is ready")
	field(DTYP, "Soft Channel")
	field(VAL,  "0")
	field(PINI, "YES")
	field(ZNAM, "0")
	field(ONAM, "1")
}


