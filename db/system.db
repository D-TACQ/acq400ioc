record (longin, "${UUT}:SYS:UPTIME") {
	field(DTYP, "longinAcqHost")
	field(INP, "@/proc/uptime")
	field(SCAN, "1 second")
}

record (stringin, "${UUT}:SYS:VERSION:SW") {
	field(DTYP, "stream")
	field(INP,  "@mb.proto getSwVersion ${SPORT}")	
	field(PINI, "YES")
}

record (stringin, "${UUT}:SYS:VERSION:FPGA" ) {
	field(DTYP, "stream")
	field(INP,  "@mb.proto getFpgaVersion ${SPORT}")	
	field(PINI, "YES")
}

record (bo, "${UUT}:NOALIASES") {
	field(DTYP, "Soft Channel")
}


