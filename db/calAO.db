record(waveform, "${UUT}:${SITE}:AO:CAL:ESLO") {
	field(DTYP, "Soft Channel")
	field(NELM, "${nchanp1}")
	field(FTVL, "FLOAT")
	field(DESC, "Slope each channel 1..N, index from 0")
}

record(waveform, "${UUT}:${SITE}:AO:CAL:EOFF") {
	field(DTYP, "Soft Channel")
	field(NELM, "${nchanp1}")
	field(FTVL, "FLOAT")
	field(DESC, "Offset each channel 1..N, index from 0")	
}

