record (bo, "${UUT}:${SITE}:HAS_CH_GAINS") {
	field(DTYP, "Soft Channel")
	field(VAL,  "${CHG}")
}

record (mbbo, "${UUT}:${SITE}:DAC_CAL") {
	field(DTYP, "Soft Channel")
	field(NOBT, "2")
	field(ZRST, "default")
	field(ONST, "caldef")
	field(TWST, "calibrated")
	field(VAL,  "2")
}
