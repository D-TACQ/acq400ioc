record (bo, "${UUT}:SFP:${CH}:TX_DISABLE") {
		field(DTYP, "boAcqHost")
		field(OUT, 	"@/dev/gpio/SFP/${CH}/TX_DISABLE")
		field(ONAM, "ON")
		field(ZNAM, "OFF")
		field(PINI, "YES")
		field(VAL,  "0")	
}

record (bo, "${UUT}:SFP:${CH}:LED:PRESENT") {
		field(DTYP, "boAcqHost")
		field(OUT, 	"@/dev/gpio/SFP/${CH}/LED")
		field(ONAM, "ON")
		field(ZNAM, "OFF")
		field(DOL,  "${UUT}:SFP:${CH}:PRESENT NPP")
		field(OMSL, "closed_loop")		
}

record (bo, "${UUT}:SFP:${CH}:LED:HB") {
field(DTYP, "boAcqHost")
		field(OUT, 	"@/dev/gpio/SFP/${CH}/HB")
		field(ONAM, "ON")
		field(ZNAM, "OFF")
		field(PINI, "YES")
		field(VAL,  "0")	
}

record(bi, "${UUT}:SFP:${CH}:PRESENT") {
		field(DTYP, "biAcqHost")
		field(INP,  "@/dev/gpio/SFP/${CH}/PRESENT")
		field(ZNAM, "FALSE")
		field(ONAM, "TRUE")
		field(SCAN, "1 second")
		field(FLNK, "${UUT}:SFP:${CH}:LED:PRESENT")
}

record(bi, "${UUT}:SFP:${CH}:LOS") {
		field(DTYP, "biAcqHost")
		field(INP,  "@/dev/gpio/SFP/${CH}/LOS")
		field(ZNAM, "FALSE")
		field(ONAM, "TRUE")
		field(SCAN, "1 second")	
}

record(bo, "${UUT}:SFP:${CH}:PWRDN") {
		field(DTYP, "stream")		
		field(OUT,  "@cpsc2.proto set_apd(${CH}) ${SPORT}")
		field(VAL,  "0")
		field(PINI, "YES")
		field(ZNAM, "FALSE")
		field(ONAM, "TRUE")		
} 

