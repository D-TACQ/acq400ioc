record (bo, "${UUT}:SFP:${CH}:TX_DISABLE") {
		field(DTYP, "boAcqHost")
		field(OUT, 	"@/dev/gpio/SFP/${CH}/TX_DISABLE")
		field(ONAM, "ON")
		field(ZNAM, "OFF")
		field(PINI, "YES")
		field(VAL,  "OFF")	
}

