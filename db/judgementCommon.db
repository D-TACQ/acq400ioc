record(longin, "${UUT}:JDG:BN")
{
   field(PINI, "0")
   field(DTYP, "asynInt32")
   field(INP,  "@asyn($(PORT),$(ADDR),$(TIMEOUT))BUFFER_NUM")
   field(SCAN, "I/O Intr")
}

record(longout, "${UUT}:JDG:MASK_FROM_DATA")
{
   field(PINI, "0")
   field(DTYP, "asynInt32")
   field(OUT,  "@asyn($(PORT),$(ADDR),$(TIMEOUT))MAKE_MASK_FROM_DATA")
   field(HOPR, "1000")
   field(LOPR,  "0")
}

record(waveform, "${UUT}:JDG:CHX:FAIL")
{
    field(PINI, "1")
    field(DTYP, "asynInt8ArrayIn")
    field(INP,  "@asyn($(PORT),$(ADDR),$(TIMEOUT))RESULT_FAIL")
    field(FTVL, "UCHAR")
    field(NELM, "$(NCHAN)")
    field(LOPR, "0")
    field(HOPR, "1")
}

record(bi, "${UUT}:JDG:OK")
{
   field(PINI, "0")
   field(DTYP, "asynInt32")
   field(INP,  "@asyn($(PORT),$(ADDR),$(TIMEOUT))OK")
   field(SCAN, "I/O Intr")
   field(ZNAM, "Fail")
   field(ONAM, "Pass")	
}