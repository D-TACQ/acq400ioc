
record(aSub, "${UUT}:${SITE}:AI:WF:TB") {
	field(SNAM, "timebase")
	
	field(FTA,  "LONG")
	field(NOA,  "1")
	field(INPA, "${UUT}:LIVE:PRE")
	
	field(FTB,  "LONG")
	field(NOB,  "1")
	field(INPB, "${UUT}:LIVE:POST")
	
	field(FTC,  "FLOAT")
	field(NOC,  "1")
	field(INPC,  "${UUT}:1:DT")
	
	field(FTVA, "FLOAT")
	field(NOVA, "${size}")
	field(DESC, "AI waveform timebase seconds")	
}

record(longin, "${UUT}:${SITE}:AI:WF:rc") {
# body of this Raw Count record provided by device-specific code
	field(SCAN, "1 second")
	field(DOL,  "${UUT}:${SITE}:AI:WF:01:UPDATES")
	field(OMSL, "closed_loop")

	field(FLNK, "${UUT}:${SITE}:AI:WF:ACTIVE")
}