record (stringin, "${UUT}:${SITE}:MODEL") {
	field(DTYP, "stream")
	field(INP, "@acq42x.proto getModel ${SPORT}")
	field(PINI, "YES")	
}

record (stringin, "${UUT}:${SITE}:PART_NUM") {
	field(DTYP, "stream")
	field(INP, "@acq42x.proto getPart ${SPORT}")
	field(PINI, "YES")
}

record (longin, "${UUT}:${SITE}:NCHAN") {
	field(DTYP, "stream")
	field(INP, "@acq42x.proto getNchan ${SPORT}")
	field(PINI, "YES")	
}

record (stringin, "${UUT}:${SITE}:SERIAL") {
	field(DTYP, "stream")
	field(INP, "@acq42x.proto getSerial ${SPORT}")
	field(PINI, "YES")	
}
record (longin, "${UUT}:${SITE}:SHOT") {
	field(DTYP, "longinAcqHost")
	field(INP, "@/dev/acq400.${SITE}.knobs/shot")
	field(SCAN, "1 second")
	field(PINI, "YES")		
}


record (bi, "${UUT}:${SITE}:RUN") {
	field(DTYP, "biAcqHost")
	field(INP, "@/dev/acq400.${SITE}.knobs/run")
	field(SCAN, "1 second")
	field(PINI, "YES")		
}

record (longout, "${UUT}:${SITE}:INTCLK_HZ") {
	field(DTYP, "Soft Channel")
	field(PINI, "YES")
	field(VAL,  "500000")
	field(DESC, "Internal FCLK [Hz]")
	field(LOPR, "10000")
	field(HOPR, "${MAXHZ}")
	field(EGU,  "Hz")
}

record(calc, "${UUT}:${SITE}:OS:FREQ") {
	field(INPA, "${UUT}:${SITE}:SIG:sample_count:FREQ")
	field(INPB, "${DECIM}")
	field(CALC, "A/B")
	field(EGU,  "Hz")
	field(DESC, "Output Sample Rate")
}

record(calc, "${UUT}:${SITE}:OS:COUNT") {
	field(INPA, "${UUT}:${SITE}:SIG:sample_count:COUNT")
	field(INPB, "${DECIM}")
	field(CALC, "A/B")
	field(DESC, "Output Sample Count")
}