record(calc, "${UUT}:${SITE}:HUDP:RX:ENABLED") {
	field(INPA, "${UUT}:${SITE}:HUDP:RX:SRC_IP")
	field(INPB, "${UUT}:${SITE}:HUDP:CTRL")
	field(INPC, "${UUT}:${SITE}:HUDP:IP")
	field(INPD, "${UUT}:${SITE}:HUDP:RX:PORT")
	field(CALC, "A!=0 && B==1 && C!=0 && D!=0")
	field(FLNK, "${UUT}:${SITE}:HUDP:TX:ENABLED PP")
}

