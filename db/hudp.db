	field(FLNK,  "${UUT}:10:HUDP:RX:ENABLED")
}

record(calc, "${UUT}:10:HUDP:RX:ENABLED") {
	field(INPA, "${UUT}:10:HUDP:RX:SRC_IP")
	field(INPB, "${UUT}:10:HUDP:CTRL")
	field(CALC, "A!=0 && B==1")
	field(FLNK, "${UUT}:10:HUDP:TX:ENABLED")
}

record(calc, "${UUT}:10:HUDP:TX:ENABLED") {
	field(INPA, "${UUT}:10:HUDP:TX:DEST_IP")
	field(INPB, "${UUT}:10:HUDP:CTRL")
	field(CALC, "A!=0 && B==1")	
}