record(aSub, "${UUT}:${SITE}:AI:WF:${cid}:V") {		
	field(INPA, "${UUT}:${SITE}:AI:WF:${cid}")
	field(INPC,  "${pmax}")
	field(INPS,  "${ESLO}")
	field(DESC,  "${desc}")	
	
	field(FTVI, "SHORT")
	field(NOVI, "${SPL_WF_LEN}")
	field(FTVJ, "SHORT")
	field(NOVJ, "${SPL_WF_LEN}")
	
}	

alias("${UUT}:${SITE}:AI:WF:${cid}:V", "${UUT}:${SITE}:AI:WF:${CH}:${P1}")


