record(waveform, "${UUT}:${SITE}:AI:WF:${ix}") {	
	field(FLNK,"${UUT}:${SITE}:AI:WF:${CH}:V")
}

record(aSub, "${UUT}:${SITE}:AI:WF:${ix}:V") {		
	field(INPA, "${UUT}:${SITE}:AI:WF:${ix}")
	field(INPC,  "${pmax}")
	field(INPS,  "${ESLO}")
	field(DESC,  "${desc}")	
}	

alias("${UUT}:${SITE}:AI:WF:${ix}:V", "${UUT}:${SITE}:AI:WF:${CH}:${P1}")


