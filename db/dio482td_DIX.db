



record(bi, "${UUT}:${SITE}:DI:${UX}") {
	field(DTYP, "Soft Channel")
	field(ZNAM, "0")
	field(ONAM, "1")
}

record(calcout, "${UUT}:${SITE}:DI:${UX}:c")
{
	field(INPA, "${UUT}:${SITE}:di32")
	field(INPB, "${IX}")
	field(CALC, "(A&(1<<B))!=0")
	field(OOPT, "Every Time")
	field(DOPT, "Use CALC")		
	field(OUT,  "${UUT}:${SITE}:DI:${UX} PP")
}

record(fanout, "${UUT}:${SITE}:di32f") {
	field(LNK${IX}, "${UUT}:${SITE}:DI:${UX}:c")
}


record(bo, "${UUT}:${SITE}:DI:${UX}:c2") {
	field(DTYP, "Soft Channel")
	field(OMSL, "closed_loop")	
	field(DOL, "${UUT}:${SITE}:DI:${UX} NPP")	
	field(OUT, "${UUT}:${SITE}:LED:DI${UX} PP")		
}

record(bi, "${UUT}:${SITE}:DI:${UX}") {
	field(FLNK, "${UUT}:${SITE}:DI:${UX}:c2")
}
